№1 
#include <stdio.h>
#include <locale.h>
int main()
{
    setlocale(LC_ALL, "Rus");
    float a, b, c;
    printf("Введите два числа:");
    scanf("%f %f", &a, &b);
    c = b;
    b = a;
    a = c;
    printf("Ответ %.3f %.3f\n", a, b);
}
№2
#include <stdio.h>
#include <locale.h>
int main()
{
    setlocale(LC_ALL, "Rus");
    float a, b, c, k;
    printf("Введите три числа:");
    scanf("%f %f %f", &a, &b, &c);
    k = a;
    a = b;
    b = c;
    c = k;
    printf("Ответ %.3f %.3f %.3f\n", a, b, c);
}
№3
#include <stdio.h>
#include <locale.h>
int main()
{
    setlocale(LC_ALL, "Rus");
    float a, b, c, k;
    printf("Введите три числа:");
    scanf("%f %f %f", &a, &b, &c);
    k = a;
    a = c;
    c = b;
    b = k;
    printf("Ответ %.3f %.3f %.3f\n", a, b, c);
}
№4
#include <stdio.h>
#include <locale.h>
#include <cmath>
int main()
{
    setlocale(LC_ALL, "Rus");
    float x, y;
    printf("Введите значение переменной x: ");
    scanf("%f", &x);
    y = (float)3 * pow(x, 6) - 6 * pow(x, 2) - 7;
    printf("Ответ %f\n", y);
}
№5
#include <stdio.h>
#include <locale.h>
#include <cmath>
int main()
{
    setlocale(LC_ALL, "Rus");
    float x, y;
    printf("Введите значение переменной x: ");
    scanf("%f", &x);
    y = (float)4 * pow((x - 3), 6) - 7 * pow((x - 3), 3) + 2;
    printf("Ответ %f\n", y);
}
№6
#include <stdio.h>
#include <locale.h>
#include <cmath>
int main()
{
    setlocale(LC_ALL, "Rus");
    float a, y;
    printf("Введите значение A: ");
    scanf("%f", &a);
     y = pow(a,5);
    rintf("Ответ %f\n", y * a * a*a);
}

№7

#include <stdio.h>
#include <locale.h>
#include <cmath>
int main()
{
    setlocale(LC_ALL, "Rus");
    float a, y, x;
    printf("Введите значение A: ");
    scanf("%f", &a);
    y = (float)pow(a, 6);
    x = (float)pow(a, 5);
    printf("Ответ %f\n", y * x * a * a * a * a);
}
